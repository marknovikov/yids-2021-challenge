{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": { "describe": true, "Folder": true },
  "ignorePatterns": ["playground.js", "source.js"],
  "rules": {
    "strict": 0,
    "no-inner-declarations": "off"
  }
}
